<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.0</TargetFramework>
    <AssemblyName>LostTech.Checkpoint</AssemblyName>
    <RootNamespace>LostTech.Checkpoint</RootNamespace>
    <Version>0.2.0</Version>
    <Authors>Victor Milovanov</Authors>
    <Company>Lost Tech LLC</Company>
    <Description>Provides asynchronous checkpoint functionality, such as asynchronous autosave for documents.</Description>
    <PackageLicenseUrl>https://github.com/losttech/Checkpoint/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/losttech/Checkpoint</PackageProjectUrl>
    <RepositoryUrl>https://github.com/losttech/Checkpoint.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>


</Project>